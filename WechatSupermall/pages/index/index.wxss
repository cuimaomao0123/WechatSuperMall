.index_swiper swiper{
  /* width: 750rpx;默认100%可以不写 */
  height: 340rpx;
}
.index_swiper image{
  width: 100%;
}

.index_cate{
  display: flex;
}
.index_cate navigator{
  flex: 1;
  padding: 20rpx;
}
.index_cate image{
  width: 100%;
}

.floor_title{
  padding: 10rpx 0;
}
.floor_title image{
  width: 100%;
}
.floor_list{
  overflow: hidden;
}
.floor_list navigator{
  float: left;
  width: 33.33%;
}
/* 后四个超链接 */
.floor_list navigator:nth-last-child(-n+4){
  /* 原图高 232*386 */
  /* 232 / 386 = 33.33vw /heigth */
  /* 第一张图片高度：height:33.3vw * 386 /232 */
  height: calc(33.3vw * 386 /232 / 2);
  border-left: 10rpx solid #fff;
}
.floor_list navigator:nth-child(2),.floor_list navigator:nth-child(3){
  border-bottom: 10rpx solid #fff;
}
.floor_list navigator image{
  width: 100%;
  height: 100%;
}